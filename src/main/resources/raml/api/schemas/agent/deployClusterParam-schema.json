{
  "title": "deploy cluster agents",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.agent.DeployClusterAgents",
  "type": "object",
  "required": [
  	"controllerId",
  	"clusterAgents"
  ],
  "properties": {
    "controllerId": {
    	"$ref": "../common/controllerIdRequired-schema.json"
    },
    "clusterAgents": {
    	"type": "array",
    	"minItems": 1,
    	"uniqueItems": true,
    	"items": {
    		"type": "object",
    		"javaType": "com.sos.joc.model.agent.DeployClusterAgent",
    		"required": [
    			"agentId",
    			"schedulingType"
    		],
    		"properties": {
    			"agentId": {
    				"$ref": "../common/string255required-schema.json"
    			},
    			"schedulingType": {
      				"$ref": "agentClusterSchedulingType-schema.json"
    			}
    		},
    		"additionalProperties": false
    	}
    },
    "auditLog": {
      "$ref": "../audit/auditParams-schema.json"
    }
  },
  "additionalProperties": false
}
