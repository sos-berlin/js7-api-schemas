{
  "title": "agent",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.agent.AgentTasks",
  "type": "object",
  "required": [
  	"agentId",
  	"agentName",
  	"url",
  	"runningTasks"
  ],
  "properties": {
    "controllerId": {
    	"$ref": "../common/filename100-schema.json"
    },
    "agentId": {
    	"minLength": 1,
    	"$ref": "../common/string255-schema.json"
    },
    "agentName": {
    	"minLength": 1,
    	"$ref": "../common/string255-schema.json"
    },
    "orderIds": {
    	"type": "array",
    	"items": {
    		"type": "string"
    	}
    },
    "runningTasks": {
    	"$ref": "../common/nonNegativeInteger-schema.json"
    },
    "url": {
    	"minLength": 1,
    	"$ref": "../common/string255-schema.json"
    },
    "isClusterWatcher": {
    	"type": "boolean",
    	"default": false
    }
  },
  "additionalProperties": false
}
