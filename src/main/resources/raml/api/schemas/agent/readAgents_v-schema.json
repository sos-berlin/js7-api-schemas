{
  "title": "read agents",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.agent.ReadAgentsV",
  "type": "object",
  "required": [
  	"controllerId"
  ],
  "properties": {
    "controllerId": {
      "$ref": "../common/controllerIdRequired-schema.json"
    },
    "agentIds": {
    	"type": "array",
    	"items": {
    		"$ref": "../common/string255required-schema.json"
    	}
    },
    "states": {
    	"type": "array",
    	"items": {
    	    "type": "string",
    		"javaType": "com.sos.joc.model.agent.AgentStateTextFilter",
            "enum": [
                "COUPLED",
                "RESETTING",
                "INITIALISED",
                "COUPLINGFAILED",
                "SHUTDOWN",
                "UNKNOWN"
            ]
    	}
    },
    "onlyVisibleAgents": {
    	"type": "boolean",
    	"default": false
    },
    "compact": {
    	"$ref": "../common/compactParam-schema.json"
    },
    "flat": {
    	"type": "boolean",
  		"default": false
    }
  },
  "additionalProperties": false
}
