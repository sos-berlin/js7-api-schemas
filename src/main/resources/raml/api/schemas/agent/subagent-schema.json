    	"$ref": "../common/string255required-schema.json"
    },
    "subagentId": {
    	"$ref": "../common/string255required-schema.json"
    },
    "url": {
    	"$ref": "../common/string255required-schema.json"
    },
    "isDirector": {
    	"$ref": "subagentDirectorType-schema.json"
    },
    "title": {
    	"$ref": "../common/string255-schema.json"
    },
    "syncState": {
    	"$ref": "../../../controller/schemas/common/syncState-schema.json"
    },
    "disabled": {
    	"type": "boolean",
    	"default": false
    },
    "deployed": {
    	"type": "boolean",
    	"default": false
    },
    "withGenerateSubagentCluster": {
    	"type": "boolean",
    	"default": false
