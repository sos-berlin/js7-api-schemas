{
  "title": "agent",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.agent.SubAgent",
  "type": "object",
  "required": [
  	"subagentId",
  	"url",
  	"isDirector"
  ],
  "properties": {
