{
	"title": "Daily Plan Order Filter Definition",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.dailyplan.DailyPlanOrderFilterDef",
	"type": "object",
	"description": "Define the filter to get orders from the daily plan",
	"required": [
		"dailyPlanDateFrom"
	],
	"properties": {
		"submissionHistoryIds": {
			"type": "array",
			"items": {
				"$ref": "../../common/nonNegativeLong-schema.json"
			}
		},
		"scheduleFolders": {
			"$ref": "../../common/folders-schema.json"
		},
		"workflowFolders": {
			"$ref": "../../common/folders-schema.json"
		},
		"schedulePaths": {
			"type": "array",
			"items": {
				"$ref": "../../common/string255-schema.json"
			}
		},
		"workflowPaths": {
			"type": "array",
			"items": {
				"$ref": "../../common/string255-schema.json"
			}
		},
		"orderIds": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"controllerIds": {
			"type": "array",
			"items": {
				"$ref": "../../common/controllerId-schema.json"
			}
		},
		"states": {
			"type": "array",
			"items": {
				"$ref": "dailyPlanOrderStateText-schema.json"
			}
		},
		"late": {
			"type": "boolean"
		},
		"dailyPlanDateFrom": {
			"$ref": "../../common/date-schema.json"
		},
		"dailyPlanDateTo": {
			"$ref": "../../common/date-schema.json"
		},
		"expandCycleOrders": {
			"$ref": "expandCycleOrdersParam-schema.json"
		},
		"auditLog": {
			"$ref": "../../audit/auditParams-schema.json"
		}
	}
}
 
