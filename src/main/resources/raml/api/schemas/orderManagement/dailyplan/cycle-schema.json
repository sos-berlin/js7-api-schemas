{
	"title": "Cyclic Order",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"javaType": "com.sos.joc.model.dailyplan.Cycle",
	"required": [
		"repeat"
	],
	"properties": {
		"begin": {
			"type": "string",
			"pattern": "^[0-9:]*$"
		},
		"end": {
			"type": "string",
			"pattern": "^[0-9:]*$"
		},
		"repeat": {
			"type": "string",
			"minLength": 1,
			"pattern": "^[0-9:]*$"
		}
	}
}
