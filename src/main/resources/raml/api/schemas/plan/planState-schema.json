{
  "title": "plan state",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.plan.PlanState",
  "type": "object",
  "required": [
    "severity",
    "_text"
  ],
  "properties": {
    "severity": {
      "type": "integer"
    },
    "_text": {
      "$ref": "planStateText-schema.json"
    },
    "since": {
      "description" : "since only for FINISHED state",
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": false
}
