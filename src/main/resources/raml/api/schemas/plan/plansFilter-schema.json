{
	"title": "PlansFilter",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.plan.PlansFilter",
	"type": "object",
	"extends": {
	    "$ref": "plansOpenCloseFilter-schema.json"
    },
	"required": [
        "controllerId"
    ],
    "properties": {
        "planSchemaIds": {
            "type": "array",
            "uniqueItems": true,
            "items": {
                "$ref": "../common/string255-schema.json"
            }
        },
        "noticeSpaceKeys": {
            "alias": "planKeys",
            "description": "Will be ignored for global schema because it has no plan keys",
            "type": "array",
            "uniqueItems": true,
            "items": {
                "$ref": "../common/string255-schema.json"
            }
        },
        "noticeBoardPaths": {
            "type": "array",
            "uniqueItems": true,
            "items": {
                "$ref": "../common/string255-schema.json"
            }
        },
        "compact": {
            "type": "boolean",
            "default": false   
        },
        "limit": {
            "description": "-1=unlimited",
            "type": "integer",
            "default": 10000
        }
    },
    "additionalProperties": false
}