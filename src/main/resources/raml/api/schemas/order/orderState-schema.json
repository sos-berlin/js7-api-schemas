{
  "title": "order state",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.order.OrderState",
  "type": "object",
  "required": [
    "severity",
    "_text"
  ],
  "properties": {
    "severity": {
      "type": "integer"
    },
    "_text": {
      "$ref": "../order/orderStateText-schema.json"
    },
    "_reason": {
      "description" : "reason for WAITING state",
      "$ref": "../order/orderWaitingReason-schema.json"
    }
  }
}
