{
  "title": "available positions for a resume",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.order.OrdersPositions",
  "type": "object",
  "required": [
  	"deliveryDate"
  ],
  "properties": {
    "deliveryDate": {
    	"$ref": "../common/timestamp-schema.json"
    },
    "surveyDate": {
    	"$ref": "../common/timestamp-schema.json"
    },
    "workflowId": {
		"$ref": "../../../controller/schemas/workflow/workflowId-schema.json"
	},
	"positions": {
     	"type": "array",
     	"uniqueItems": true,
     	"items": {
     		"type": "object",
     		"javaType": "com.sos.joc.model.order.Position",
     		"properties": {
     			"position": {
     				"$ref": "../../../controller/schemas/common/position-schema.json"
    			},
				"positionString": {
     				"$ref": "../common/string255-schema.json"
    			},
				"type": {
     				"$ref": "../common/string255-schema.json"
    			},
				"label": {
					"$ref": "../common/string255-schema.json"
    			}
     		}
     	}
    }
  }
}
