{
  "title": "available positions for a resume",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.order.OrdersPositions",
  "type": "object",
  "required": [
  	"deliveryDate",
    "orders"
  ],
  "properties": {
    "deliveryDate": {
    	"$ref": "../common/timestamp-schema.json"
    },
    "surveyDate": {
    	"$ref": "../common/timestamp-schema.json"
    },
    "orderIds": {
        "type": "array",
        "uniqueItems": true,
        "items": {
        	"$ref": "../common/string255-schema.json"
        }
    },
    "workflowId": {
		"$ref": "../../../controller/schemas/workflow/workflowId-schema.json"
	},
	"positions": {
     	"type": "array",
     	"uniqueItems": true,
     	"items": {
     		"type": "object",
     		"javaType": "com.sos.joc.model.order.Positions",
     		"properties": {
     			"position": {
     				"$ref": "../../../controller/schemas/common/position-schema.json"
    			},
				"positionString": {
     				"$ref": "../common/string255-schema.json"
    			}
     		}
     	}
    },
    "disabledPositionChange": {
    	"$ref": "positionChange-schema.json"
    },
    "variables": {
      "description": "variables of the current position; only if one orderId is set",
      "$ref": "../../../inventory/schemas/common/variables-schema.json"
    }
  }
}
