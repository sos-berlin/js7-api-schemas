{
	"title": "post notices",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.board.PostExpectedNotices",
	"type": "object",
	"required": [
		"controllerId",
		"expectedNotices"
	],
	"properties": {
		"controllerId": {
			"$ref": "../common/controllerIdRequired-schema.json"
		},
		"expectedNotices": {
			"type": "array",
			"uniqueItems": true,
			"minItems": 1,
			"items": {
				"type": "object",
				"javaType": "com.sos.joc.model.board.ExpectedNoticesPerBoard",
				"required": [
					"noticeBoardPath",
					"workflowPaths"
				],
				"properties": {
					"noticeBoardPath": {
						"$ref": "../common/string255required-schema.json"	
					},
					"workflowPaths": {
						"type": "array",
						"uniqueItems": true,
						"minItems": 1,
						"items": {
							"$ref": "../common/string255required-schema.json"
						}	
					}			
				}
			},
			"additionalProperties": false
		},
		"endOfLife": {
			"$ref": "../common/dateFromTo-schema.json"
		},
		"timeZone": {
			"$ref": "../common/timezone-schema.json"
		},
		"auditLog": {
			"$ref": "../audit/auditParams-schema.json"
		}
	},
	"additionalProperties": false
}