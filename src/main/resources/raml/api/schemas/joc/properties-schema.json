{
  "title": "properties",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.Properties",
  "type": "object",
  "required": [
    "deliveryDate",
    "forceCommentsForAuditLog",
    "comments",
    "showViews",
    "securityLevel",
    "defaultProfileAccount",
    "apiVersion",
    "inventoryVersion",
    "displayFoldersInViews",
    "jocVersion",
    "kioskRole"
  ],
  "properties": {
    "deliveryDate": {
      "$ref": "../common/timestamp-schema.json"
    },
    "title": {
      "type": "string"
    },
    "securityLevel": {
      "$ref": "../common/jocSecurityLevel-schema.json"
    },
    "defaultProfileAccount": {
      "type": "string"
    },
    "apiVersion": {
      "type": "string"
    },
    "inventoryVersion": {
      "type": "string"
    },
    "forceCommentsForAuditLog": {
      "type": "boolean",
      "default": false
    },
    "comments": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "copy": {
      "$ref": "suffixPrefix-schema.json"
    },
    "restore": {
      "$ref": "suffixPrefix-schema.json"
    },
    "import": {
      "$ref": "suffixPrefix-schema.json"
    },
    "showViews": {
      "javaType": "com.sos.joc.model.ShowViewProperties",
      "type": "object",
      "properties": {
        "dashboard": {
          "type": "boolean"
        },
        "dailyPlan": {
          "type": "boolean"
        },
        "workflows": {
          "type": "boolean"
        },
        "orders": {
          "type": "boolean"
        },
        "jobStreams": {
          "type": "boolean"
        },
        "fileTransfers": {
          "type": "boolean"
        },
        "resources": {
          "type": "boolean"
        },
        "history": {
          "type": "boolean"
        },
        "auditLog": {
          "type": "boolean"
        },
        "conditions": {
          "type": "boolean"
        },
        "configuration": {
          "type": "boolean"
        },
        "monitor": {
          "type": "boolean"
        },
        "report": {
          "type": "boolean"
        }
      }
    },
    "displayFoldersInViews":  {
      "type": "boolean",
      "default": true
    },
    "welcomeDoNotRemindMe": {
      "type": "boolean",
      "default": false
    },
    "welcomeGotIt": {
      "type": "boolean",
      "default": false
    },
    "allowEmptyArguments": {
      "type": "boolean",
      "default": false
    },
    "allowUndeclaredVariables": {
      "type": "boolean",
      "default": false
    },
    "numOfTagsDisplayedAsOrderId": {
      "$ref": "../common/nonNegativeInteger-schema.json"
    },
    "numOfWorkflowTagsDisplayed": {
      "$ref": "../common/nonNegativeInteger-schema.json"
    },
    "clusterLicense": {
      "type": "boolean",
      "default": false
    },
    "licenseValidFrom": {
      "$ref": "../common/timestamp-schema.json"
    },
    "licenseValidUntil": {
      "$ref": "../common/timestamp-schema.json"
    },
    "licenseType": {
      "$ref": "licenseType-schema.json"
    },
    "kioskRole": {
      "type": "string"
    }
  },
  "additionalProperties": false
}
