{
  "title": "controllerIds",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.jobscheduler.ControllerIds",
  "type": "object",
  "required": [
    "deliveryDate",
    "controllerIds",
    "selected"
  ],
  "properties": {
    "deliveryDate": {
      "$ref": "../common/timestamp-schema.json"
    },
    "controllerIds": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "selected": {
      "description": "The Id from the 'controllerIds' collection which is specified in the selected field will be used for all further calls",
      "type": "string"
    },
    "precedence": {
      "description": "primary (0), secondary (1), ...only relevant for passive cluster",
      "$ref": "../common/nonNegativeInteger-schema.json"
    }
  }
}
