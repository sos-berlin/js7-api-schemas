{
	"title": "order history log entry",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.history.order.OrderLogEntry",
	"type": "object",
	"required": [
		"controllerDatetime",
		"orderId",
		"logLevel",
		"logEvent",
		"position"
	],
	"properties": {
		"controllerDatetime": {
			"type": "string"
		},
		"agentDatetime": {
			"type": "string"
		},
		"orderId": {
			"type": "string"
		},
		"logLevel": {
			"type": "string"
		},
		"logEvent": {
			"$ref": "../../../../controller/schemas/event/eventType-schema.json"
		},
		"position": {
			"type": "string"
		},
		"agentId": {
			"alias": "agentPath",
			"type": "string"
		},
		"agentName": {
			"type": "string"
		},
		"agentUrl": {
			"type": "string"
		},
		"subagentClusterId": {
			"type": "string"
		},
		"job": {
			"type": "string"
		},
		"taskId": {
			"type": "number",
			"format": "utc-millisec",
			"minimum": 0
		},
		"returnCode": {
			"type": "number",
			"format": "utc-millisec"
		},
		"error": {
			"type": "object",
			"javaType": "com.sos.joc.model.history.order.OrderLogEntryError",
			"properties": {
				"errorState": {
					"type": "string"
				},
				"errorReason": {
					"type": "string"
				},
				"errorCode": {
					"type": "string"
				},
				"errorText": {
					"type": "string"
				}
			},
			"additionalProperties": false
		},
		"locks": {
			"type": "array",
			"items": {
				"type": "object",
				"javaType": "com.sos.joc.model.history.order.Lock",
				"required": [
					"lockName",
					"limit"
				],
				"properties": {
					"lockName": {
						"alias": "lockId,lockPath",
						"type": "string"
					},
					"limit": {
						"type": "integer",
						"minimum": 0,
						"default": 1
					},
					"count": {
						"type": "integer"
					},
					"lockState": {
						"type": "object",
						"javaType": "com.sos.joc.model.history.order.LockState",
						"properties": {
							"orderIds": {
								"type": "string"
							},
							"queuedOrderIds": {
								"type": "string"
							}
						},
						"additionalProperties": false
					}
				}
			},
			"additionalProperties": false
		},
		"expectNotices": {
			"type": "object",
			"javaType": "com.sos.joc.model.history.order.ExpectNotices",
			"oneOf": [
				{
					"required": [
						"consumed"
					]
				},
				{
					"required": [
						"waitingFor"
					]
				}
			],
			"properties": {
				"consumed": {
					"type": "string"
				},
				"waitingFor": {
					"type": "array",
					"items": {
						"type": "object",
						"javaType": "com.sos.joc.model.history.order.ExpectNotice",
						"required": [
							"boardName",
							"id"
						],
						"properties": {
							"boardName": {
								"type": "string"
							},
							"id": {
								"type": "string"
							}
						}
					},
					"additionalProperties": false
				}
			},
			"additionalProperties": false
		},
		"postNotice": {
			"type": "object",
			"javaType": "com.sos.joc.model.history.order.PostNotice",
			"required": [
				"boardName",
				"id",
				"endOfLife"
			],
			"properties": {
				"boardName": {
					"type": "string"
				},
				"id": {
					"type": "string"
				},
				"endOfLife": {
					"type": "string"
				}
			},
			"additionalProperties": false
		},
		"retrying": {
			"type": "object",
			"javaType": "com.sos.joc.model.history.order.Retrying",
			"properties": {
				"delayedUntil": {
					"type": "string"
				}
			},
			"additionalProperties": false
		},
		"caught": {
			"type": "object",
			"javaType": "com.sos.joc.model.history.order.Caught",
			"required": [
				"cause"
			],
			"properties": {
				"cause": {
					"$ref": "orderLogEntry-caughtCause-schema.json"
				}
			},
			"additionalProperties": false
		}
	},
	"additionalProperties": false
}