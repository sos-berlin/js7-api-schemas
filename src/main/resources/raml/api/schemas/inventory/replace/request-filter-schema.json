{
	"title": "filter for replace",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.inventory.replace.RequestFilter",
	"type": "object",
	"extends" : {
    	"$ref": "../common/request-filter-schema.json"
  	},
	"anyOf": [
        {
            "required": [
                "id",
                "search",
                "replace"
            ]
        },
        {
            "required": [
                "path",
                "objectType",
                "search",
                "replace"
            ]
        }
    ],
	"properties": {
		"search": {
			"type": "string",
			"pattern": "^[^/]*$",
  			"maxLength": 255
		},
		"replace": {
			"type": "string",
			"pattern": "^[^/]*$",
  			"maxLength": 255
		}
	}
}
