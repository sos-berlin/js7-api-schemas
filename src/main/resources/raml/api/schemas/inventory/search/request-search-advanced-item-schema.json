{
	"title": "Inventory advanced search",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.inventory.search.RequestSearchAdvancedItem",
	"type": "object",
	"properties": {
		"agentName": {
			"$ref": "../../common/string255-schema.json"
		},
		"workflow": {
			"$ref": "../../common/string255-schema.json"
		},
		"fileOrderSource": {
			"$ref": "../../common/string255-schema.json"
		},
		"lock": {
			"$ref": "../../common/string255-schema.json"
		},
		"schedule": {
			"$ref": "../../common/string255-schema.json"
		},
		"includeScript": {
			"alias": "includeScripts",
			"type": "string",
  			"pattern": "^[^<>]*$",
  			"maxLength": 255
		},
		"calendar": {
			"$ref": "../../common/string255-schema.json"
		},
		"noticeBoard": {
			"alias": "noticeBoards",
			"type": "string",
  			"pattern": "^[^<>]*$",
  			"maxLength": 255
		},
		"jobResource": {
			"alias": "jobResources",
			"type": "string",
  			"pattern": "^[^<>]*$",
  			"maxLength": 255
