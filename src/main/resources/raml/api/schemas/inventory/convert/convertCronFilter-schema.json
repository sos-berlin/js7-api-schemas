{
	"title": "Convert Cron Filter",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.inventory.convert.ConvertCronFilter",
	"type": "object",
	"required": [
		"calendarName",
		"agentName"
	],
	"properties": {
		"folder": {
			"type": "string",
			"pattern": "^(/|(/[^/\\\\<>?:\"|*]+)+)$",
			"maxLength": 255,
			"minLength": 1,
			"default": "/"
		},
		"calendarName": {
			"$ref": "../../common/string255-schema.json"
		},
		"agentName": {
			"$ref": "../../common/string100-schema.json"
		},
		"systemCrontab": {
			"type": "boolean",
			"default": false
		},
		"auditLog": {
			"$ref": "../../audit/auditParams-schema.json"
		}
	}
}