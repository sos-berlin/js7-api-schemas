{
	"title": "Filter Deployables",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.inventory.deploy.DeployablesFilter",
	"type": "object",
	"required": [
		"folder"
	],
	"properties": {
		"folder": {
			"$ref": "../../common/path-schema.json"
		},
		"recursive": {
		   "type": "boolean",
		   "default": false
		},
		"objectTypes": {
			"type": "array",
			"items": {
				"$ref": "../common/configurationType-schema.json"
			}
		},
		"onlyValidObjects": {
			"type": "boolean",
			"default": false
		},
		"withRemovedObjects": {
			"type": "boolean",
			"default": false
		},
		"withoutDrafts": {
			"type": "boolean",
			"default": false
		},
		"withoutDeployed": {
			"type": "boolean",
			"default": false
		},
		"latest": {
			"description": "only relevant for deployed objects",
			"type": "boolean",
			"default": false
		}
	}
}
