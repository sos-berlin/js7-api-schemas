{
    "title": "parameter for a report run",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "javaType": "com.sos.joc.model.reporting.RunFilter",
    "type": "object",
    "required": [
        "templateId",
        "frequencies"    
    ],
    "properties": {
        "templateId": {
            "$ref": "../common/nonNegativeInteger-schema.json"
        },
        "frequencies": {
            "type": "array",
            "uniqueItems": true,
            "minItems": 1,
            "items": {
                "$ref": "frequency-schema.json"
            }
        },
        "size": {
            "$ref": "../common/nonNegativeInteger-schema.json"
        },
        "dateFrom": {
            "$ref": "../common/date-schema.json"
            
        }
    },
    "additionalProperties": false
}
