{
  "title": "agent",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.joc.model.report.Agent",
  "type": "object",
  "required": [
    "agent",
    "cause",
    "numOfSuccessfulTasks",
    "numOfJobs"
  ],
  "properties": {
    "controllerId": {
      "type": "string"
    },
    "agent": {
      "description": "Url of an Agent",
      "type": "string",
      "pattern": "https?://.+"
    },
    "numOfSuccessfulTasks": {
      "$ref": "../common/nonNegativeLong-schema.json"
    },
    "numOfJobs": {
      "$ref": "../common/nonNegativeLong-schema.json"
    }
  }
}
