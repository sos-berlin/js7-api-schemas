{
	"title": "ExportFolderForSigningFilter",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.publish.folder.ExportFolderForSigning",
	"type": "object",
  "required": [
      "controllerId",
      "objectTypes",
      "folders"
  ],
 	"properties": {
    "controllerId": {
      "$ref": "../../common/string255-schema.json"
    },
    "objectTypes" : {
      "type" : "array",
      "items" : {
        "$ref": "../../inventory/common/configurationType-schema.json"
      }
    },
    "folders" : {
      "type" : "array",
      "items" : {
        "$ref": "../../common/path-schema.json"
      }
    },
    "recursive": {
      "type": "boolean",
      "default": false
    },
    "onlyValidObjects": {
      "type": "boolean",
      "default": false
    },
    "withoutDrafts": {
      "type": "boolean",
      "default": false
    },
    "withoutDeployed": {
      "type": "boolean",
      "default": false
    }
	}
}
