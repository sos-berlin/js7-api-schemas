{
	"title": "Filter for environment related Objects",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.publish.repository.EnvRelatedConfigurations",
	"type": "object",
	"anyOf": [{
		"required": ["draftConfigurations"]
	}, {
		"required": ["deployConfigurations"]
	}, {
		"required": ["releasedConfigurations"]
	}],
	"properties": {
		"draftConfigurations": {
			"type": "array",
			"items": {
				"$ref": "../config-schema.json"
			}
		},
	    "deployConfigurations": {
	      	"type" : "array",
	      	"items" : {
	        	"$ref": "../config-schema.json"
	      	}
	    },
		"releasedConfigurations": {
			"type": "array",
			"items": {
				"$ref": "../config-schema.json"
			}
		}
	}
}