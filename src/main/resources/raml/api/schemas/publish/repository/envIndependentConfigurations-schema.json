{
	"title": "Filter for environment independent Objects",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.publish.repository.EnvIndependentConfigurations",
	"type": "object",
	"anyOf": [{
			"required": ["draftConfigurations"]
		}, {
			"required": ["deployConfigurations"]
		}
	],
	"properties": {
		"draftConfigurations": {
			"type": "array",
			"items": {
				"$ref": "../config-schema.json"
			}
		},
		"deployConfigurations": {
			"type": "array",
			"items": {
				"$ref": "../config-schema.json"
			}
		}
	}
}