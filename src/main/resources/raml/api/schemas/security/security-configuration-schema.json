{
	"title": "Authentication configuration",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.security.SecurityConfiguration",
	"type": "object",
	"required": [
		"identityServiceName"
	],
	"properties": {
		"deliveryDate": {
			"$ref": "../common/timestamp-schema.json"
		},
		"identityServiceName": {
			"$ref": "../common/string255-schema.json"
		},
		"main": {
			"type": "array",
			"items": {
				"type": "object",
				"javaType": "com.sos.joc.model.security.SecurityConfigurationMainEntry",
				"required": [
					"entryName",
					"entryValue"
				],
				"properties": {
					"entryName": {
						"type": "string"
					},
					"entryValue": {
						"type": "array",
						"items": {
							"type": "string"
						}
					},
					"entryComment": {
						"type": "array",
						"items": {
							"type": "string"
						}
					}
				},
				"additionalProperties": false
			}
		},
		"accounts": {
			"type": "array",
			"items": {
				"type": "object",
				"javaType": "com.sos.joc.model.security.SecurityConfigurationAccount",
				"required": [
					"account"
				],
				"properties": {
					"account": {
						"type": "string"
					},
					"password": {
						"type": "string"
					},
					"hashedPassword": {
						"type": "string"
					},
					"repeatedPassword": {
						"type": "string"
					},
					"oldPassword": {
						"type": "string"
					},
					"forcePasswordChange": {
						"$ref": "forcePasswordChange-schema.json"
					},
					"disabled": {
						"$ref": "disabledParam-schema.json"
					},
					"identityServiceId": {
						"$ref": "../common/nonNegativeLong-schema.json"
					},
					"roles": {
						"type": "array",
						"items": {
							"type": "string"
						}
					}
				},
				"additionalProperties": false
			}
		},
		"profiles": {
			"type": "array",
			"items": {
				"type": "object",
				"javaType": "com.sos.joc.model.configuration.Profile",
				"properties": {
					"account": {
						"type": "string"
					},
					"lastLogin": {
						"$ref": "../common/timestamp-schema.json"
					}
				},
				"additionalProperties": false
			}
		},
		"roles": {
			"javaType": "com.sos.joc.model.security.permissions.SecurityConfigurationRoles",
			"type": "object",
			"additionalProperties": {
				"$ref": "role-schema.json"
			}
		}
	},
	"additionalProperties": false
}