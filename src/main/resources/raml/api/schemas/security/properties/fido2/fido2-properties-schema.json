{
	"title": "FIDO2 Connect Properties",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"javaType": "com.sos.joc.model.security.properties.fido2.Fido2Properties",
	"type": "object",
	"properties": {
		"iamFido2UserVerification": {
			"$ref": "fido2UserVerification-schema.json",
			"default":"PREFERRED"
		},
		"iamFido2Timeout": {
			"$ref": "../../../common/nonNegativeInteger-schema.json",
			"default":60
		},
		"iamFido2ResidentKey": {
			"$ref": "fido2ResidentKey-schema.json"
		},
		"iamFido2Transports": {
			"type": "array",
			"items": {
			"$ref": "fido2Transports-schema.json"
			}
		},		
		"requireAccount": {
			"type": "boolean",
  			"default": false
		},
		"iamFido2EmailSettings": {
			"$ref": "fido2EmailSettings-schema.json"
		}
	},
	"additionalProperties": false
}
    